from datetime import date
dataNascimento = int(input('Digite sua data de nascimento: '))
calculoIdade = (date.today().year - dataNascimento)
if calculoIdade <= 9:
    print('Com sua idade de {} anos, você está na categoria mirim de natação.'.format(calculoIdade))
elif calculoIdade <= 14:
    print('Com sua idade de {} anos, você está na categoria infantil de natação.'.format(calculoIdade))
elif calculoIdade <= 19:
    print('Com sua idade de {} anos, você está na categoria junior de natação.'.format(calculoIdade))
elif calculoIdade <= 25:
    print('Com sua idade de {} anos, você está na categoria sênior de natação.'.format(calculoIdade))
elif calculoIdade > 25:
    print('Com sua idade de {} anos, você está na categoria master de natação.'.format(calculoIdade))